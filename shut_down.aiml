        <template>
            <srai>SHUTDOWN</srai>
        </template>
    </category>

    <category>
        <pattern>* TURN OFF *</pattern>
        <template>
            <srai>SHUTDOWN</srai>
        </template>
    </category>

    <category>
        <pattern>SHUT DOWN</pattern>
        <template>
            <srai>SHUTDOWN</srai>
        </template>
    </category>

    <category>
        <pattern>* SHUT DOWN</pattern>
        <template>
            <srai>SHUTDOWN</srai>
        </template>
    </category>

    <category>
        <pattern>SHUT DOWN *</pattern>
        <template>
